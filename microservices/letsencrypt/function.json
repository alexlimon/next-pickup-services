{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "letsencrypt/.well-known/acme-challenge/{file}",
      "methods": [
        "get",
        "post"
      ]  
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
